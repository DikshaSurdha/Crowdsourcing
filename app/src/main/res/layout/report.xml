<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/image4"
    android:weightSum="1">

    <TextView
        android:text="HOW ARE YOU FEELING TODAY!!!"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:layout_weight="0.07"
        android:textStyle="normal|bold"
        android:textSize="20sp"
        android:layout_marginTop="38dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="27dp"
        android:textColor="@color/common_google_signin_btn_text_dark_focused" />

    <CheckBox
        android:text="Dengue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_alignStart="@+id/textView"
        android:layout_marginTop="23dp"
        android:textSize="18sp"
        android:id="@+id/checkBox"
        android:textStyle="normal|bold" />

    <CheckBox
        android:text="Chikungunya"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/checkBox"
        android:layout_alignStart="@+id/checkBox"
        android:layout_marginTop="49dp"
        android:textSize="18sp"
        android:id="@+id/checkBox2"
        android:textStyle="normal|bold" />

    <CheckBox
        android:text="Malaria"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/checkBox2"
        android:textSize="18sp"
        android:id="@+id/checkBox3"
        android:textStyle="normal|bold" />

    <CheckBox
        android:text="Chicken pox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/checkBox4"
        android:layout_alignStart="@+id/checkBox4"
        android:layout_marginTop="39dp"
        android:textSize="18sp"
        android:id="@+id/checkBox5"
        android:textStyle="normal|bold" />

    <CheckBox
        android:text="CheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/checkBox5"
        android:layout_alignStart="@+id/checkBox9"
        android:textSize="18sp"
        android:id="@+id/checkBox10"
        android:textStyle="normal|bold" />

    <CheckBox
        android:text="Cholera"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/checkBox8"
        android:layout_alignBaseline="@+id/checkBox3"
        android:layout_alignBottom="@+id/checkBox3"
        android:textSize="18sp"
        android:layout_alignStart="@+id/checkBox7"
        android:textStyle="normal|bold" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/checkBox7"
        android:text="Tuberculosis"
        android:layout_alignBaseline="@+id/checkBox2"
        android:layout_alignBottom="@+id/checkBox2"
        android:layout_alignParentEnd="true"
        android:textSize="18sp"
        android:layout_marginEnd="49dp"
        android:textStyle="normal|bold" />

    <CheckBox
        android:text="Flu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/checkBox6"
        android:layout_above="@+id/checkBox2"
        android:textSize="18sp"
        android:layout_alignStart="@+id/checkBox7"
        android:textStyle="normal|bold" />

    <CheckBox
        android:text="Conjunctivitis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="42dp"
        android:textSize="18sp"
        android:id="@+id/checkBox4"
        android:layout_below="@+id/checkBox3"
        android:layout_alignStart="@+id/checkBox3"
        android:textStyle="normal|bold" />

    <CheckBox
        android:text="Gastroenteritis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/checkBox9"
        android:textSize="18sp"
        android:layout_alignBaseline="@+id/checkBox4"
        android:layout_alignBottom="@+id/checkBox4"
        android:layout_alignStart="@+id/checkBox8"
        android:textStyle="normal|bold" />

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button2"
        android:textSize="18sp"
        android:layout_alignBottom="@+id/checkBox9"
        android:layout_alignParentStart="true"
        android:layout_marginStart="167dp" />

</RelativeLayout>